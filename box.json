{
    "name":"cbcommerce-admin",
    "version":"1.0.0",
    "author":"Ortus Solutions, Corp",
    "location":"https://downloads.ortussolutions.com/ortussolutions/coldbox-modules/cbcommerce-admin/@build.version@/cbcommerce-admin-@build.version@+@build.number@.zip",
    "homepage":"https://cbcommerce.ortusbooks.com",
    "documentation":"https://cbcommerce.ortusbooks.com",
    "repository":{
        "type":"git",
        "URL":"https://github.com/coldbox-modules/cbcommerce-api"
    },
    "bugs":"https://github.com/coldbox-modules/cbcommerce-api/issues",
    "slug":"cbCommerce",
    "shortDescription":"Admin module for the cbCommerce e-commerce platform",
    "description":"Admin module for the cbCommerce e-commerce platform",
    "type":"modules",
    "dependencies":{
        "coldbox-asset-bag":"^1.0.3",
        "cbi18n":"^3.2.0"
    },
    "devDependencies":{},
    "installPaths":{
        "coldbox-asset-bag":"modules/coldbox-asset-bag/",
        "cbi18n":"modules/cbi18n/"
    },
    "scripts":{
        "harness:install":"install --force && cd test-harness && install --force"
    },
    "ignore":[
        "test-harness",
        "*/.md"
    ],
    "cfmigrations":{
        "connectionInfo":{
            "password":"${DB_PASSWORD}",
            "connectionString":"${DB_CONNECTIONSTRING}",
            "class":"${DB_CLASS}",
            "username":"${DB_USER}"
        },
        "schema":"${DB_DATABASE}",
        "defaultGrammar":"MySQLGrammar"
    }
}